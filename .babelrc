{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@Components": "./src/components",
          "@Config": "./configuration",
          "@Definitions": "./src/models/definitions",
          "@Interfaces": "./src/models/interfaces",
          "@Pages": "./pages",
          "@Redux": "./src/store",
          "@Services": "./src/services",
          "@Server": "./server",
          "@Static": "./public/static",
          "@Utils": "./src/utils"
        }
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ]
  ]
}
